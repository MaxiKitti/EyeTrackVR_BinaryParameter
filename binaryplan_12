                try:
                    xpercentage = (((xt - vc.xmin) * 100) / (vc.xmax - vc.xmin)) / 100 #TESTING NEEDED AM UNSURE IF VALUES NEED TO BE FLIPPED
                    ypercentage = (((yt - vc.ymin) * 100) / (vc.ymax - vc.ymin)) / 100
                    
                    #all binary parameter booleans mapped for the left blink
                    
                    if LBlink <= 0.25:
                      LBlink1 = false
                      LBlink2 = false
                    elif LBlink > 0.25 and LBlink <= 0.5:
                      LBlink1 = true
                      LBlink2 = false
                    elif LBlink > 0.5 and LBlink <= 0.75:
                      LBlink1 = false
                      LBlink2 = true
                    elif LBlink > 0.75 and LBlink <= 1:
                      LBlink1 = true
                      LBlink2 = true
                      
                      #all binary parameter booleans mapped for the right blink
                      
                    if RBlink <= 0.25:
                      RBlink1 = false
                      RBlink2 = false
                    elif RBlink > 0.25 and RBlink <= 0.5:
                      RBlink1 = true
                      RBlink2 = false
                    elif RBlink > 0.5 and RBlink <= 0.75:
                      RBlink1 = false
                      RBlink2 = true
                    elif RBlink > 0.75 and RBlink <= 1:
                      RBlink1 = true
                      RBlink2 = true
                
                    client.send_message("/Avatar/LBlink1", LBlink1)
                    client.send_message("/Avatar/LBlink2", LBlink2)
                    client.send_message("/Avatar/RBlink1", RBlink1)
                    client.send_message("/Avatar/RBlink2", RBlink2)
                    print('L: ', LBlink, ' R: ', RBlink)
