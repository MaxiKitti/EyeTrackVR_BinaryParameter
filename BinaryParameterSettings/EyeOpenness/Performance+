#all binary parameter booleans mapped for the left eye openness
                    
if LBlink <= 1 / 2:
    LBlink1 = false
elif LBlink > 1 / 2 and LBlink <= 1:
    LBlink1 = true
#all binary parameter booleans mapped for the right eye openness

if RBlink <= 1 / 2:
    RBlink1 = false
elif RBlink > 1 / 2 and RBlink <= 1:
    RBlink1 = true
                
client.send_message("/Avatar/LBlink1", LBlink1)
client.send_message("/Avatar/RBlink1", RBlink1)
print('L: ', LBlink, ' R: ', RBlink)
